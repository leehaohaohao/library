<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lihao.dao.CategoryDao">
    <select id="select" resultType="BookCategory">
        select * from t_category where category_id = #{category_id}
    </select>
    <select id="select_by_name" resultType="BookCategory">
        select * from t_category where category = #{category}
    </select>
    <select id="select_all_category" resultType="BookCategory">
        select * from t_category
    </select>
    <select id="select_by_id" resultType="BookCategory">
        select * from t_category where category_id = #{category_id}
    </select>
</mapper>